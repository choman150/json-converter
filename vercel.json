{
    "headers": [
        {
            "source": "/sitemap.xml",
            "headers": [
                { "key": "Content-Type", "value": "application/xml" },
                { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "www.choman150.click"
                }
            ],
            "destination": "https://choman150.click/:1",
            "statusCode": 301
        }
    ]
}
